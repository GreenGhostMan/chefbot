<launch>

  <arg name="twist_to_motors" value="0"/>

  <include file="$(find chefbot_bringup)/launch/urdf.launch" />

  <node name="arduino_node" pkg="chefbot_bringup" type="driver_imu_disable_debug.py" output="screen">
    <param name="port" value="/dev/ttyACM0" />
    <param name="baudRate" value="115200" />
  </node>


  <group if="$(eval twist_to_motors == 1)">
    <include file="$(find chefbot_bringup)/launch/twist.launch" />
  </group>

</launch>
